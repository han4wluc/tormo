    "experimentalDecorators": true,
    "target": "es5",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "lib"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/index.ts"
  ]
}
